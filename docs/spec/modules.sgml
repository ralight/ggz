    <refsect1>
      <title>Description</title>
      <para>Request to join a game</para>
      <refsect2>
	<title>Message Data</title>
	<variablelist>
	  <varlistentry>
	    <term><emphasis role="bold">SEAT</emphasis></term>
	    <listitem>
	      <para>
		Seat number of the joining player
	      </para>
	    </listitem>
	  </varlistentry>
	  <varlistentry>
	    <term><emphasis role="bold">NAME</emphasis></term>
	    <listitem>
	      <para>
		The name of the affected player
	      </para>
	    </listitem>
	  </varlistentry>
	  <varlistentry>
	    <term><emphasis role="bold">FD</emphasis></term>
	    <listitem>
	      <para>
		File descriptor used for communication with the player
	      </para>
	    </listitem>
	  </varlistentry>
	</variablelist>

      </refsect2>

      <refsect2>
	<title>Usage</title>
	<para>
