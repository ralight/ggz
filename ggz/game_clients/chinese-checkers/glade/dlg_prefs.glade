<?xml version="1.0"?>
<GTK-Interface>

<project>
  <name>dlg_prefs</name>
  <program_name>dlg_prefs</program_name>
  <directory></directory>
  <source_directory>src</source_directory>
  <pixmaps_directory>pixmaps</pixmaps_directory>
  <language>C</language>
  <gnome_support>False</gnome_support>
  <gettext_support>False</gettext_support>
  <use_widget_names>True</use_widget_names>
  <output_main_file>False</output_main_file>
  <output_support_files>False</output_support_files>
  <output_build_files>False</output_build_files>
  <main_source_file>prefs_dlg.c</main_source_file>
  <main_header_file>prefs_dlg.h</main_header_file>
  <handler_source_file>prefs_cb.c</handler_source_file>
  <handler_header_file>prefs_cb.h</handler_header_file>
</project>

<widget>
  <class>GtkDialog</class>
  <name>dlg_prefs</name>
  <title>Chinese Checkers Preferences</title>
  <type>GTK_WINDOW_TOPLEVEL</type>
  <position>GTK_WIN_POS_NONE</position>
  <modal>False</modal>
  <allow_shrink>True</allow_shrink>
  <allow_grow>True</allow_grow>
  <auto_shrink>False</auto_shrink>

  <widget>
    <class>GtkVBox</class>
    <child_name>Dialog:vbox</child_name>
    <name>dialog-vbox1</name>
    <homogeneous>False</homogeneous>
    <spacing>0</spacing>

    <widget>
      <class>GtkHBox</class>
      <child_name>Dialog:action_area</child_name>
      <name>dialog-action_area1</name>
      <border_width>10</border_width>
      <homogeneous>True</homogeneous>
      <spacing>5</spacing>
      <child>
	<padding>0</padding>
	<expand>False</expand>
	<fill>True</fill>
	<pack>GTK_PACK_END</pack>
      </child>

      <widget>
	<class>GtkButton</class>
	<name>ok_button</name>
	<width>48</width>
	<tooltip>Save changes and dismiss preferences dialog</tooltip>
	<can_focus>True</can_focus>
	<signal>
	  <name>clicked</name>
	  <handler>on_ok_button_clicked</handler>
	  <last_modification_time>Sun, 11 Mar 2001 08:10:18 GMT</last_modification_time>
	</signal>
	<label>OK</label>
	<child>
	  <padding>0</padding>
	  <expand>False</expand>
	  <fill>False</fill>
	</child>
      </widget>

      <widget>
	<class>GtkButton</class>
	<name>apply_button</name>
	<width>48</width>
	<tooltip>Save changes, keeping preferences dialog open</tooltip>
	<can_focus>True</can_focus>
	<signal>
	  <name>clicked</name>
	  <handler>on_apply_button_clicked</handler>
	  <last_modification_time>Sun, 11 Mar 2001 08:10:25 GMT</last_modification_time>
	</signal>
	<label>Apply</label>
	<child>
	  <padding>0</padding>
	  <expand>False</expand>
	  <fill>False</fill>
	</child>
      </widget>

      <widget>
	<class>GtkButton</class>
	<name>cancel_button</name>
	<width>48</width>
	<tooltip>Cancel changes since the last apply</tooltip>
	<can_focus>True</can_focus>
	<signal>
	  <name>clicked</name>
	  <handler>on_cancel_button_clicked</handler>
	  <last_modification_time>Sun, 11 Mar 2001 08:10:32 GMT</last_modification_time>
	</signal>
	<label>Cancel</label>
	<child>
	  <padding>0</padding>
	  <expand>False</expand>
	  <fill>False</fill>
	</child>
      </widget>
    </widget>

    <widget>
      <class>GtkVBox</class>
      <name>vbox1</name>
      <homogeneous>False</homogeneous>
      <spacing>0</spacing>
      <child>
	<padding>0</padding>
	<expand>True</expand>
	<fill>True</fill>
      </child>

      <widget>
	<class>GtkFrame</class>
	<name>frame1</name>
	<label>General Preferences</label>
	<label_xalign>0</label_xalign>
	<shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
	<child>
	  <padding>0</padding>
	  <expand>False</expand>
	  <fill>False</fill>
	</child>

	<widget>
	  <class>GtkCheckButton</class>
	  <name>check_beep</name>
	  <border_width>2</border_width>
	  <can_focus>True</can_focus>
	  <label>Beep on our turn</label>
	  <active>False</active>
	  <draw_indicator>True</draw_indicator>
	</widget>
      </widget>

      <widget>
	<class>GtkFrame</class>
	<name>frame2</name>
	<label>Board Theme Directory</label>
	<label_xalign>0</label_xalign>
	<shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
	<child>
	  <padding>0</padding>
	  <expand>True</expand>
	  <fill>True</fill>
	</child>

	<widget>
	  <class>GtkHBox</class>
	  <name>hbox1</name>
	  <homogeneous>False</homogeneous>
	  <spacing>0</spacing>

	  <widget>
	    <class>GtkEntry</class>
	    <name>dir_entry</name>
	    <width>225</width>
	    <tooltip>Enter a directory pathname which contains a Chinese Checkers theme graphics set</tooltip>
	    <can_focus>True</can_focus>
	    <editable>True</editable>
	    <text_visible>True</text_visible>
	    <text_max_length>0</text_max_length>
	    <text>/usr/local/lib/ggz/ccheckers/pixmaps/default</text>
	    <child>
	      <padding>0</padding>
	      <expand>True</expand>
	      <fill>True</fill>
	    </child>
	  </widget>

	  <widget>
	    <class>GtkVButtonBox</class>
	    <name>vbuttonbox1</name>
	    <layout_style>GTK_BUTTONBOX_DEFAULT_STYLE</layout_style>
	    <spacing>10</spacing>
	    <child_min_width>85</child_min_width>
	    <child_min_height>27</child_min_height>
	    <child_ipad_x>7</child_ipad_x>
	    <child_ipad_y>0</child_ipad_y>
	    <child>
	      <padding>0</padding>
	      <expand>True</expand>
	      <fill>True</fill>
	    </child>

	    <widget>
	      <class>GtkButton</class>
	      <name>browse_button</name>
	      <tooltip>Open a directory browser to locate a theme directory</tooltip>
	      <can_default>True</can_default>
	      <can_focus>True</can_focus>
	      <signal>
		<name>clicked</name>
		<handler>on_browse_button_clicked</handler>
		<last_modification_time>Sun, 11 Mar 2001 08:16:31 GMT</last_modification_time>
	      </signal>
	      <label>Browse</label>
	    </widget>

	    <widget>
	      <class>GtkButton</class>
	      <name>default_button</name>
	      <tooltip>Click to reset default theme graphics</tooltip>
	      <can_default>True</can_default>
	      <can_focus>True</can_focus>
	      <signal>
		<name>clicked</name>
		<handler>on_default_button_clicked</handler>
		<last_modification_time>Mon, 12 Mar 2001 03:09:55 GMT</last_modification_time>
	      </signal>
	      <label>Default</label>
	    </widget>
	  </widget>
	</widget>
      </widget>
    </widget>
  </widget>
</widget>

</GTK-Interface>
