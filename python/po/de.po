#: ../ggzboard/ggzboard:1033
msgid "Options:"
msgstr "Optionen:"

#: ../ggzboard/ggzboard:1034
msgid "[-h | --help           ] Display this game help"
msgstr "[-h | --help           ] Diesen Hilfebildschirm anzeigen"

#: ../ggzboard/ggzboard:1035
msgid "[-v | --version        ] Show version information"
msgstr "[-v | --version        ] Versionsinformationen anzeigen"

#: ../ggzboard/ggzboard:1036
msgid "[-g | --ggz            ] Request game in GGZ mode"
msgstr "[-g | --ggz            ] Spiel im GGZ-Modus starten"

#: ../ggzboard/ggzboard:1037
msgid "[-G | --game <gamename>] Skip intro screen and launch game directly"
msgstr ""
"[-G | --game <gamename>] Einführungsbildschirm überspringen und Spiel starten"

#: ../ggzboard/ggzboard:1038
msgid "[-f | --fullscreen     ] Start in fullscreen mode"
msgstr "[-f | --fullscreen     ] Spiel im Vollbildmodus starten"

