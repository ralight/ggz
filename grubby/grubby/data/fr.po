#: ../modules/self.c:100
#, c-format
msgid ""
"I'm %s, your favorite chat bot!\n"
"I'm here to answer your question, and learn more about you.\n"
"You may type '%s help ' to get to know what I understand.\n"
"Have fun :-)"
msgstr ""
"Je suis %s, ton bot préféré!\n"
"Je peux repondre à tes questions, et apprendre des choses sur toi.\n"
"Pour savoir ce que je comprends, tu devrais taper '%s help''.\n"
"Bonne chance :-)"

#: ../modules/self.c:112
msgid "This is the list of public commands I understand:\n"
msgstr "Ceci est la liste des commaneds publiques que je comprends:\n"

#: ../modules/self.c:133
msgid "about: let me tell about myself\n"
msgstr "about: laisse moi te raconter ma vie\n"

#: ../modules/self.c:134
msgid "uptime: disclose my age\n"
msgstr "uptime: révelle mon age\n"

#: ../modules/self.c:138
msgid "shut up: make me quiet\n"
msgstr "shut up: me faire taire\n"

#: ../modules/self.c:143
msgid "do i have any messages: let me look up if there's news for you\n"
msgstr "do i have any messages: laisse moi regarder s'il y a des informations pour toi\n"

#: ../modules/self.c:145
msgid "tell <nick> <message>: store message for player nick\n"
msgstr "tell <pseudo> <message>: enregistre un message pour un joueur du nom de <pseudo>\n"

#: ../modules/self.c:150
msgid "who is <nick>: request information about nick\n"
msgstr "who is <pseudo>: demande des informations à propos de pseudo\n"

#: ../modules/self.c:152
msgid "have you seen <nick>: ask me when I last saw nick\n"
msgstr "have you seen <pseudo>: Me demande quand est-ce que j'ai vu pseudo pour la derniere fois\n"

#: ../modules/self.c:154
msgid "my (email | pager) is <contact>: register contact information\n"
msgstr "my (email | pager) is <contact>: enregistre les informations de mise en contact\n"

#: ../modules/self.c:156
msgid "my name is <fullname>: register you as fullname\n"
msgstr "my name is <nom complet>: Enregistrez votre nom complet\n"

#: ../modules/self.c:160
msgid "join my game: make me join your table\n"
msgstr "join my game: me fait rejoindre votre table\n"

#: ../modules/self.c:164
msgid "<thing> is <explanation>: Learn a fact\n"
msgstr "<chose> est <explication>: Apprendre un fait\n"

#: ../modules/self.c:165
msgid "what is <thing>: Get information about a fact\n"
msgstr "what is <chose>: Obtenir des informations à propos de quelque chose\n"

#: ../modules/self.c:171
msgid "i am from <countrycode>: register language\n"
msgstr "i am from <code de pays>: Enregistrer la langue\n"

