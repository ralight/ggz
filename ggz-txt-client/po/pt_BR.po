#: ../ggz-txt/input.c:133
#, c-format
msgid "--- Client version: %s"
msgstr "--- Versão do cliente: %s"

#: ../ggz-txt/input.c:136
#, c-format
msgid "--- Unknown command, try %chelp."
msgstr "--- Comando inconhecido, tente %chelp."

#: ../ggz-txt/input.c:246 ../ggz-txt/input.c:322 ../ggz-txt/input.c:361
msgid "You must connect to a server first."
msgstr "Você tem que conectar para algum servidor primeiro."

#: ../ggz-txt/input.c:291
msgid "List what?"
msgstr "Mostrar o quê?"

#: ../ggz-txt/input.c:303
#, c-format
msgid "Joining a %s"
msgstr "Entrar num %s"

#: ../ggz-txt/input.c:311
msgid "Join what?"
msgstr "Entrar em o quê?"

#: ../ggz-txt/input.c:332
msgid "Join which room?"
msgstr "Entrar qual sala?"

#: ../ggz-txt/input.c:347
msgid "Describe which room?"
msgstr "Explicar qual sala?"

#: ../ggz-txt/input.c:365
msgid "You must join a room first."
msgstr "Você tem que entrar numa sala primeiro."

#: ../ggz-txt/input.c:412
msgid "Send message to which table?"
msgstr "Enviar mensagem para qual mesa?"

#: ../ggz-txt/input.c:445
msgid "You must be in a room to launch a game."
msgstr "Você tem que estar numa sala para lançar um jogo."

#: ../ggz-txt/input.c:451
msgid "No game types defined for this room. Maybe try 'list types'?"
msgstr "Não há nenhum jogo definido nesta sala. Talvez queria tentar 'list types'?"

#: ../ggz-txt/input.c:459
#, c-format
msgid "Launching game of %s, (%s-%s)"
msgstr "Lançando jogo de %s, (%s-%s)"

#: ../ggz-txt/input.c:472
#, c-format
msgid "No suitable game modules (out of %i) defined for that game."
msgstr "Não achi um módulo de jogo (de %i em total) para este tipo de jogo."

#: ../ggz-txt/input.c:473
#, c-format
msgid "Download one from %s."
msgstr "Baixe um de %s."

#: ../ggz-txt/input.c:478
#, c-format
msgid "Found %i game modules, using frontend '%s'."
msgstr "Achei %s módulos de jogo, uso interface '%s'."

#: ../ggz-txt/input.c:517
msgid "Join which table?"
msgstr "Entrar qual mesa?"

#: ../ggz-txt/input.c:531
msgid "That table doesn't exist! Maybe try '/list tables'?"
msgstr "Esta mesa não existe! Talvez queria tentar '/list tables'?"

#: ../ggz-txt/main.c:137
#, c-format
msgid "The GGZ Gaming Zone Text Client\n"
msgstr "O cliente text da GGZ Gaming Zone\n"

#: ../ggz-txt/main.c:138
#, c-format
msgid ""
"http://www.ggzgamingzone.org/clients/console/\n"
"\n"
msgstr ""
"http://www.ggzgamingzone.org/clients/console/\n"
"\n"

#: ../ggz-txt/main.c:139
#, c-format
msgid "Call: ggz-txt [options] [uri]:\n"
msgstr "Usa: ggz-txt [opções] [uri]:\n"

#: ../ggz-txt/main.c:140
#, c-format
msgid "-r / --reverse: Use reverse colours\n"
msgstr "-r / --reverse: Trocar as cores\n"

#: ../ggz-txt/main.c:141
#, c-format
msgid "-h / --help: Display this help\n"
msgstr "-h / --help: Mostrar esta ajuda\n"

#: ../ggz-txt/main.c:142
#, c-format
msgid "-v / --version: Display version information\n"
msgstr "-v / --version: Mostrar informações sobre a versão\n"

#: ../ggz-txt/main.c:221
#, c-format
msgid "Startup command: %s"
msgstr "Comando da execução: %s"

#: ../ggz-txt/output.c:96
msgid "--- GGZ Gaming Zone -- Help"
msgstr "--- GGZ Gaming Zone -- Ajuda"

#: ../ggz-txt/output.c:97
msgid "--- -----------------------"
msgstr "--- -----------------------"

#: ../ggz-txt/output.c:98
msgid "---"
msgstr "---"

#: ../ggz-txt/output.c:99
msgid "--- /connect [<server>[:<port>]] [<nick>] [<password>]"
msgstr "--- /connect [<servidor>[:<port>]] [<apelido>] [<senha>]"

#: ../ggz-txt/output.c:100
msgid "---    Connect to a GGZ server"
msgstr "---    Conectar para um servidor GGZ"

#: ../ggz-txt/output.c:101
msgid "--- /disconnect                             Disconnect from server"
msgstr "--- /disconnect                             Desconectar do servidor"

#: ../ggz-txt/output.c:102
msgid "--- /exit                                   Quit GGZ Gaming Zone"
msgstr "--- /exit                                   Sair da GGZ Gaming Zone"

#: ../ggz-txt/output.c:103
msgid ""
"--- /desc <room>                            Get description of room <room>"
msgstr ""
"--- /desc <sala>                            Recebe descrição da sala <sala>"

#: ../ggz-txt/output.c:104
msgid "--- /join room|table <num>                  Join room or table <num>"
msgstr "--- /join room|table <num>                  Entre na sala ou mesa <num>"

#: ../ggz-txt/output.c:105
msgid ""
"--- /list players|rooms|tables|types        List the requested information."
msgstr ""
"--- /list players|rooms|tables|types        Mostre as informações específicos."

#: ../ggz-txt/output.c:106
msgid "--- /msg <player> <msg>                     Send a msg to a player"
msgstr "--- /msg <player> <msg>                     Envie uma msg para um jogador"

#: ../ggz-txt/output.c:107
msgid "--- /table <msg>                            Send a msg to your table"
msgstr "--- /table <msg>                            Envie uma msg para a sua mesa"

#: ../ggz-txt/output.c:108
msgid "--- /beep <player>                          Beep player <player>"
msgstr "--- /beep <jogador>                         Notificar jogador <jogador>"

#: ../ggz-txt/output.c:109
msgid "--- /launch                                 Launch a game if possible"
msgstr "--- /launch                                 Lançar um jogo se possível"

#: ../ggz-txt/output.c:110
msgid "--- /version                                Display the client version"
msgstr "--- /version                                Mostre a versão do cliente"

#: ../ggz-txt/output.c:111
msgid ""
"--- /who                                    List current player in the room"
msgstr ""
"--- /who                                    Mostre os jogadores nesta sala"

#: ../ggz-txt/output.c:112
msgid ""
"--- /wall <msg>                             Admin only command, broadcast to "
"all rooms"
msgstr ""
"--- /wall <msg>                             Comando admin, envie para todas "
"as salas"

#: ../ggz-txt/output.c:119
msgid "Welcome to the text-only GGZ client!\n"
msgstr "Bem-vindo no cliente do ambiente text de GGZ!\n"

#: ../ggz-txt/output.c:120
msgid "--Written by Brent Hendricks & Justin Zaun  (C) 2000\n"
msgstr "--Escrito por Brent Hendricks & Justin Zaun  (C) 2000\n"

