#: ../../ggzcore/netxml.c:1279
msgid "Unknown error"
msgstr "Erreur inconnue"

#: ../../ggzcore/server.c:888
msgid "Unknown negotiation"
msgstr "Négociation inconnue"

#: ../../ggzcore/server.c:915
msgid "Already logged in"
msgstr "Déjà connecté"

#: ../../ggzcore/server.c:918
msgid "The password was incorrect"
msgstr "Le mot de passe est incorrect"

#: ../../ggzcore/server.c:921
msgid "Name is already taken"
msgstr "Nom déjà utilisé"

#: ../../ggzcore/server.c:924
msgid "This name is already registered so cannot be used by a guest"
msgstr ""
"Ce nom est déjà enregistré et ne peut donc pas être utilisé par un "
"invité"

#: ../../ggzcore/server.c:928
msgid "No such name was found"
msgstr "Aucun nom semblable trouvé"

#: ../../ggzcore/server.c:931
msgid "Name too long"
msgstr "Nom trop long"

