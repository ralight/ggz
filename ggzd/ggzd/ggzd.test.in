# GGZ conf file for testing in builddir.  Do *not* use this file as a
# system-wide configuration for ggz.  Use ggzd.conf instead.

[General]
ServerName = A Test Server

[Directories]
ConfDir = @packagesrcdir@/test
GameDir = @packagesrcdir@/test
DataDir = @packagesrcdir@/test

[Logs]
LogFile = @packagesrcdir@/ggzd.log
LogTypes = All
DebugFile = @packagesrcdir@/ggzd.debug
DebugTypes = All

[Files]
MOTD = ggzd.motd
