[GameInfo]
Author = Rich Gade
Description = GGZ Cards game module for La Pocha
Homepage = http://www.ggzgamingzone.org/gameservers/ggzcards/
Name = GGZCards-LaPocha
Version = @GGZCARDS_VERSION@

[LaunchInfo]
ExecutablePath = ggzd.ggzcards --game=lapocha
# Set ExecutableArgs in the room file

[Protocol]
Engine = @GGZCARDS_PROTOCOL_ENGINE@
Version = @GGZCARDS_PROTOCOL_VERSION@

[TableOptions]
AllowLeave = 1
BotsAllowed = 1..3
PlayersAllowed = 4
KillWhenEmpty = 0
AllowSpectators = 1

[Statistics]
Records = 1
Ratings = 1
