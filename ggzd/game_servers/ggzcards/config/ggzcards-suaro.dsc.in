[GameInfo]
Author = Jason Short
Description = GGZ Cards game module for Suaro
Homepage = http://www.ggzgamingzone.org/gameservers/ggzcards/
Name = GGZCards-Suaro
Version = @GGZCARDS_VERSION@

[LaunchInfo]
ExecutablePath = ggzd.ggzcards --game=suaro
# Set ExecutableArgs in the room file

[Protocol]
Engine = @GGZCARDS_PROTOCOL_ENGINE@
Version = @GGZCARDS_PROTOCOL_VERSION@

[TableOptions]
AllowLeave = 1
BotsAllowed = 1
PlayersAllowed = 2
KillWhenEmpty = 0
AllowSpectators = 1

[Statistics]
Records = 1
Ratings = 1
