msgid "/table <message> .......... Message to your table"
msgstr "/table <메세지> ............ 테이블에 메세지"

#: ../ggz-gtk/chat.c:93
msgid "/wall <message> ........... Admin command"
msgstr "/wall <메세지> ............. 관리 명령"

#: ../ggz-gtk/chat.c:95
msgid "/beep <username> .......... Beep a player"
msgstr "/beep <사용자이름> ......... 플레이어에게 삑 소리 내기"

#: ../ggz-gtk/chat.c:97
msgid "/help ..................... Get help"
msgstr "/help ...................... 도움말 보기"

