#: ../ggz-txt/input.c:139
#, c-format
msgid "--- Client version: %s"
msgstr "--- Client-Version: %s"

#: ../ggz-txt/input.c:142
#, c-format
msgid "--- Unknown command, try %chelp."
msgstr "--- Unbekannter Befehl, propiere %chelp."

#: ../ggz-txt/input.c:262 ../ggz-txt/input.c:368 ../ggz-txt/input.c:411
#: ../ggz-txt/input.c:457 ../ggz-txt/input.c:634
msgid "You must connect to a server first."
msgstr "Du mußt zu einem Server verbunden sein."

#: ../ggz-txt/input.c:316
msgid "List what?"
msgstr "Was auflisten?"

#: ../ggz-txt/input.c:326 ../ggz-txt/input.c:338
msgid "Join what?"
msgstr "Was betreten?"

#: ../ggz-txt/input.c:330
#, c-format
msgid "Joining a %s"
msgstr "Betrete einen %s"

#: ../ggz-txt/input.c:379
#, c-format
msgid "Room %s does not exist."
msgstr "Der Raum %s existiert nicht."

#: ../ggz-txt/input.c:382
msgid "Join which room?"
msgstr "Welchen Raum betreten?"

#: ../ggz-txt/input.c:397
msgid "Describe which room?"
msgstr "Welchen Raum beschreiben?"

#: ../ggz-txt/input.c:415 ../ggz-txt/input.c:461
msgid "You must join a room first."
msgstr "Du mußt zuerst einen Raum betreten."

#: ../ggz-txt/input.c:449
msgid "Error: Missing player name."
msgstr "Fehler: Der Spielername muss angegeben werden."

#: ../ggz-txt/input.c:450 ../ggz-txt/input.c:471
msgid "Syntax: /msg <player> <msg>"
msgstr "Syntax: /msg <spieler> <nachricht>"

#: ../ggz-txt/input.c:470
msgid "Error: Missing message."
msgstr "Fehler: Fehlende Nachricht."

#: ../ggz-txt/input.c:481
msgid "Send message to which table?"
msgstr "Nachricht an welchem Tisch senden?"

#: ../ggz-txt/input.c:514
msgid "You must be in a room to launch a game."
msgstr "Du mußt in einem Raum sein um ein Spiel starten zu können."

#: ../ggz-txt/input.c:520
msgid "No game types defined for this room. Maybe try 'list types'?"
msgstr ""
"Es sind keine Spieltypen für diesen Raum definiert. Probiere mal 'list "
"types'."

#: ../ggz-txt/input.c:528
#, c-format
msgid "Launching game of %s, (%s-%s)"
msgstr "Starte Spiel %s (%s-%s)"

#: ../ggz-txt/input.c:541
#, c-format
msgid "No suitable game modules (out of %i) defined for that game."
msgstr ""
"Es sind keine Spielmodule (von %i verfügbaren) für dieses Spiel geeignet."

#: ../ggz-txt/input.c:542
#, c-format
msgid "Download one from %s."
msgstr "Lade eines von %s herunter."

#: ../ggz-txt/input.c:547
#, c-format
msgid "Found %i game modules, using frontend '%s'."
msgstr "%i Spielmodule gefunden, nutze Frontend '%s'."

#: ../ggz-txt/input.c:586
msgid "Join which table?"
msgstr "An welchem Tisch teilnehmen?"

#: ../ggz-txt/input.c:600
msgid "That table doesn't exist! Maybe try '/list tables'?"
msgstr "Dieser Tisch existiert nicht. Probiere mal 'list tables'."

#: ../ggz-txt/input.c:663
msgid "Kicking a player requires a reason."
msgstr "Die Aussperrung eines Spielers benötigt die Angabe eines Grundes."

#: ../ggz-txt/input.c:667
msgid "Unknown admin command."
msgstr "Unbekanntes Admin-Kommando."

#: ../ggz-txt/main.c:141
#, c-format
msgid "The GGZ Gaming Zone Text Client\n"
msgstr "Der GGZ Gaming Zone Text-Client\n"

#: ../ggz-txt/main.c:142
#, c-format
msgid ""
"http://www.ggzgamingzone.org/clients/console/\n"
"\n"
msgstr ""
"http://www.ggzgamingzone.org/clients/console/\n"
"\n"

#: ../ggz-txt/main.c:143
#, c-format
msgid "Call: ggz-txt [options] [uri]:\n"
msgstr "Aufruf: ggz-txt [optionen] [uri]:\n"

#: ../ggz-txt/main.c:144
#, c-format
msgid "-r / --reverse: Use reverse colours\n"
msgstr "-r / --reverse: Verwendet reverses Farbschema\n"

#: ../ggz-txt/main.c:145
#, c-format
msgid "-h / --help: Display this help\n"
msgstr "-h / --help: Zeigt diese Hilfe an\n"

#: ../ggz-txt/main.c:146
#, c-format
msgid "-v / --version: Display version information\n"
msgstr "-v / --version: Zeigt Versionsinformationen\n"

#: ../ggz-txt/main.c:240
#, c-format
msgid "Startup command: %s"
msgstr "Startkommando: %s"

#: ../ggz-txt/output.c:109
msgid "--- GGZ Gaming Zone -- Help"
msgstr "--- GGZ Gaming Zone -- Hilfe"

#: ../ggz-txt/output.c:110
msgid "--- -----------------------"
msgstr "--- -----------------------"

#: ../ggz-txt/output.c:111
msgid "---"
msgstr "---"

#: ../ggz-txt/output.c:112
msgid "--- /connect [<server>[:<port>][:tls]] [<nick>] [<password>]"
msgstr "--- /connect [<server>[:<port>][:tls]] [<nick>] [<passwort>]"

#: ../ggz-txt/output.c:113
msgid "---    Connect to a GGZ server and log in"
msgstr "---    Verbindet zu einem GGZ-Server und loggt ein"

#: ../ggz-txt/output.c:114
msgid "--- /disconnect                                  Disconnect from server"
msgstr "--- /disconnect                         Trennt die Verbindung"

#: ../ggz-txt/output.c:115
msgid "--- /exit                                        Quit GGZ Gaming Zone"
msgstr "--- /exit                               GGZ Gaming Zone verlassen"

#: ../ggz-txt/output.c:116
msgid ""
"--- /desc <room>                                 Get description of room "
"<room>"
msgstr ""
"--- /desc <raum>                        Beschreibung von Raum <raum> bekommen"

#: ../ggz-txt/output.c:117
msgid ""
"--- /join room|table <num>                       Join room or table <num>"
msgstr ""
"--- /join room|table <num>              Raum oder Tisch <num> betreten"

#: ../ggz-txt/output.c:118
msgid ""
"--- /list allrooms|players|rooms|tables|types    List the requested "
"information."
msgstr ""
"--- /list allrooms|players|rooms|tables|types Listet die gewünschten "
"Informationen auf"

#: ../ggz-txt/output.c:119
msgid "--- /msg <player> <msg>                          Send a msg to a player"
msgstr ""
"--- /msg <spieler> <nachricht>          Sendet eine Nachricht zu einem "
"Spieler"

#: ../ggz-txt/output.c:120
msgid ""
"--- /table <msg>                                 Send a msg to your table"
msgstr ""
"--- /table <nachricht>                  Sendet eine Nachricht an deinen "
"Tisch"

#: ../ggz-txt/output.c:121
msgid "--- /beep <player>                               Beep player <player>"
msgstr ""
"--- /beep <spieler>                     Sendet einen Piepton an einen "
"Spieler"

#: ../ggz-txt/output.c:122
msgid ""
"--- /launch                                      Launch a game if possible"
msgstr ""
"--- /launch                             Startet (falls möglich) ein Spiel"

#: ../ggz-txt/output.c:123
msgid ""
"--- /version                                     Display the client version"
msgstr ""
"--- /version                            Zeigt die Client-Versionsnummer "
"an"

#: ../ggz-txt/output.c:124
msgid ""
"--- /who                                         List current player in the "
"room"
msgstr ""
"--- /who                                Listet die derzeitigen Spieler "
"im Raum auf"

#: ../ggz-txt/output.c:125
msgid ""
"--- /wall <msg>                                  Admin only command, "
"broadcast to all rooms"
msgstr ""
"--- /wall <nachricht>                   Adminbefehl: Broadcast zu allen "
"Räumen"

#: ../ggz-txt/output.c:126
msgid ""
"--- /log <logfile>                               Log conversation into "
"logfile"
msgstr ""
"--- /log <logdatei>                     Zeichnet die Gespräche in einer "
"Datei auf"

#: ../ggz-txt/output.c:127
msgid ""
"--- /admin ...                                   Administrative commands"
msgstr ""
"--- /admin ...                          Administrative Kommandos"

#: ../ggz-txt/output.c:128
msgid ""
"---    ... gag <player>                          Global ignore on messages "
"from <player>"
msgstr ""
"---    ... gag <spieler>                Ingnoriert alle Nachrichten "
"von <spieler>"

#: ../ggz-txt/output.c:129
msgid "---    ... ungag <player>                        Revert gagging"
msgstr "---    ... ungag <spieler>              Zeigt die ignorierten "
"Nachrichten wieder an"

#: ../ggz-txt/output.c:130
msgid ""
"---    ... kick <player> <reason>                Kick <player> from server"
msgstr ""
"---    ... kick <spieler> <grund>       Wirft <spieler> vom Server"

