#: ../ggz-txt/input.c:139
#, c-format
msgid "--- Client version: %s"
msgstr "--- Versão do cliente: %s"

#: ../ggz-txt/input.c:142
#, c-format
msgid "--- Unknown command, try %chelp."
msgstr "--- Comando inconhecido, tente %chelp."

#: ../ggz-txt/input.c:262 ../ggz-txt/input.c:368 ../ggz-txt/input.c:411
#: ../ggz-txt/input.c:457 ../ggz-txt/input.c:634
msgid "You must connect to a server first."
msgstr "Você tem que conectar para algum servidor primeiro."

#: ../ggz-txt/input.c:316
msgid "List what?"
msgstr "Mostrar o quê?"

#: ../ggz-txt/input.c:326 ../ggz-txt/input.c:338
msgid "Join what?"
msgstr "Entrar em o quê?"

#: ../ggz-txt/input.c:330
#, c-format
msgid "Joining a %s"
msgstr "Entrar num %s"

#: ../ggz-txt/input.c:379
#, c-format
msgid "Room %s does not exist."
msgstr "A sala %s não existe."

#: ../ggz-txt/input.c:382
msgid "Join which room?"
msgstr "Entrar qual sala?"

#: ../ggz-txt/input.c:397
msgid "Describe which room?"
msgstr "Explicar qual sala?"

#: ../ggz-txt/input.c:415 ../ggz-txt/input.c:461
msgid "You must join a room first."
msgstr "Você tem que entrar numa sala primeiro."

#: ../ggz-txt/input.c:449
msgid "Error: Missing player name."
msgstr "Erro: Falta o nome do jogador."

#: ../ggz-txt/input.c:450 ../ggz-txt/input.c:471
msgid "Syntax: /msg <player> <msg>"
msgstr "Syntax: /msg <jogador> <msg>"

#: ../ggz-txt/input.c:470
msgid "Error: Missing message."
msgstr "Erro: Falta a mensagem."

#: ../ggz-txt/input.c:481
msgid "Send message to which table?"
msgstr "Enviar mensagem para qual mesa?"

#: ../ggz-txt/input.c:514
msgid "You must be in a room to launch a game."
msgstr "Você tem que estar numa sala para lançar um jogo."

#: ../ggz-txt/input.c:520
msgid "No game types defined for this room. Maybe try 'list types'?"
msgstr ""
"Não há nenhum jogo definido nesta sala. Talvez queria tentar 'list types'?"

#: ../ggz-txt/input.c:528
#, c-format
msgid "Launching game of %s, (%s-%s)"
msgstr "Lançando jogo de %s, (%s-%s)"

#: ../ggz-txt/input.c:541
#, c-format
msgid "No suitable game modules (out of %i) defined for that game."
msgstr "Não achi um módulo de jogo (de %i em total) para este tipo de jogo."

#: ../ggz-txt/input.c:542
#, c-format
msgid "Download one from %s."
msgstr "Baixe um de %s."

#: ../ggz-txt/input.c:547
#, c-format
msgid "Found %i game modules, using frontend '%s'."
msgstr "Achei %s módulos de jogo, uso interface '%s'."

#: ../ggz-txt/input.c:586
msgid "Join which table?"
msgstr "Entrar qual mesa?"

#: ../ggz-txt/input.c:600
msgid "That table doesn't exist! Maybe try '/list tables'?"
msgstr "Esta mesa não existe! Talvez queria tentar '/list tables'?"

#: ../ggz-txt/input.c:663
msgid "Kicking a player requires a reason."
msgstr "Travar um jogador para fora requere uma razão."

#: ../ggz-txt/input.c:667
msgid "Unknown admin command."
msgstr "Comando admin desconhecido."

#: ../ggz-txt/main.c:141
#, c-format
msgid "The GGZ Gaming Zone Text Client\n"
msgstr "O cliente text da GGZ Gaming Zone\n"

#: ../ggz-txt/main.c:142
#, c-format
msgid ""
"http://www.ggzgamingzone.org/clients/console/\n"
"\n"
msgstr ""
"http://www.ggzgamingzone.org/clients/console/\n"
"\n"

#: ../ggz-txt/main.c:143
#, c-format
msgid "Call: ggz-txt [options] [uri]:\n"
msgstr "Usa: ggz-txt [opções] [uri]:\n"

#: ../ggz-txt/main.c:144
#, c-format
msgid "-r / --reverse: Use reverse colours\n"
msgstr "-r / --reverse: Trocar as cores\n"

#: ../ggz-txt/main.c:145
#, c-format
msgid "-h / --help: Display this help\n"
msgstr "-h / --help: Mostrar esta ajuda\n"

#: ../ggz-txt/main.c:146
#, c-format
msgid "-v / --version: Display version information\n"
msgstr "-v / --version: Mostrar informações sobre a versão\n"

#: ../ggz-txt/main.c:240
#, c-format
msgid "Startup command: %s"
msgstr "Comando da execução: %s"

#: ../ggz-txt/output.c:109
msgid "--- GGZ Gaming Zone -- Help"
msgstr "--- GGZ Gaming Zone -- Ajuda"

#: ../ggz-txt/output.c:110
msgid "--- -----------------------"
msgstr "--- -----------------------"

#: ../ggz-txt/output.c:111
msgid "---"
msgstr "---"

#: ../ggz-txt/output.c:112
msgid "--- /connect [<server>[:<port>][:tls]] [<nick>] [<password>]"
msgstr "--- /connect [<servidor>[:<port>][:tls]] [<apelido>] [<senha>]"

#: ../ggz-txt/output.c:113
msgid "---    Connect to a GGZ server and log in"
msgstr "---    Conectar para um servidor GGZ e autenticar nele"

#: ../ggz-txt/output.c:114
msgid "--- /disconnect                                  Disconnect from server"
msgstr "--- /disconnect                             Desconectar do servidor"

#: ../ggz-txt/output.c:115
msgid "--- /exit                                        Quit GGZ Gaming Zone"
msgstr "--- /exit                                   Sair da GGZ Gaming Zone"

#: ../ggz-txt/output.c:116
msgid ""
"--- /desc <room>                                 Get description of room "
"<room>"
msgstr ""
"--- /desc <sala>                            Recebe descrição da sala <sala>"

#: ../ggz-txt/output.c:117
msgid ""
"--- /join room|table <num>                       Join room or table <num>"
msgstr ""
"--- /join room|table <num>                  Entre na sala ou mesa <num>"

#: ../ggz-txt/output.c:118
msgid ""
"--- /list allrooms|players|rooms|tables|types    List the requested "
"information."
msgstr ""
"--- /list allrooms|players|rooms|tables|types Mostre as informações "
"específicos."

#: ../ggz-txt/output.c:119
msgid "--- /msg <player> <msg>                          Send a msg to a player"
msgstr "--- /msg <jogador> <msg>                    Envie uma msg para um jogador"

#: ../ggz-txt/output.c:120
msgid ""
"--- /table <msg>                                 Send a msg to your table"
msgstr ""
"--- /table <msg>                            Envie uma msg para a sua mesa"

#: ../ggz-txt/output.c:121
msgid "--- /beep <player>                               Beep player <player>"
msgstr ""
"--- /beep <jogador>                         Notificar jogador <jogador>"

#: ../ggz-txt/output.c:122
msgid ""
"--- /launch                                      Launch a game if possible"
msgstr "--- /launch                                 Lançar um jogo se possível"

#: ../ggz-txt/output.c:123
msgid ""
"--- /version                                     Display the client version"
msgstr "--- /version                                Mostre a versão do cliente"

#: ../ggz-txt/output.c:124
msgid ""
"--- /who                                         List current player in the "
"room"
msgstr ""
"--- /who                                    Mostre os jogadores nesta sala"

#: ../ggz-txt/output.c:125
msgid ""
"--- /wall <msg>                                  Admin only command, "
"broadcast to all rooms"
msgstr ""
"--- /wall <msg>                             Comando admin, envie para todas "
"as salas"

#: ../ggz-txt/output.c:126
msgid ""
"--- /log <logfile>                               Log conversation into "
"logfile"
msgstr ""
"--- /log <arquivo>                          Escreve a conversação para um "
"aquivo"

#: ../ggz-txt/output.c:127
msgid ""
"--- /admin ...                                   Administrative commands"
msgstr ""
"--- /admin ...                              Comando administrativo"

#: ../ggz-txt/output.c:128
msgid ""
"---    ... gag <player>                          Global ignore on messages "
"from <player>"
msgstr ""
"---    ... gag <jogador>                    Ignorar mensagens do jogador"

#: ../ggz-txt/output.c:129
msgid "---    ... ungag <player>                        Revert gagging"
msgstr "---    ... ungag <jogador>                  Acabar a ignorar"

#: ../ggz-txt/output.c:130
msgid ""
"---    ... kick <player> <reason>                Kick <player> from server"
msgstr ""
"---    ... kick <jogador> <razão>           Trava <jogador> do servidor"

