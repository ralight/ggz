INC = -I/usr/local/qt3/include
LIB = -L/usr/local/qt3/lib -lqt-mt

all:
	moc -o qmetagui.moc.cpp qmetagui.h
	g++ -c qmetagui.moc.cpp $(INC)
	g++ -c qmetagui.cpp $(INC)
	g++ -c main.cpp $(INC)
	g++ -o metagui qmetagui.o qmetagui.moc.o main.o $(LIB)

clean:
	rm -f metagui
	rm -f *.o
	rm -f *.moc.cpp

