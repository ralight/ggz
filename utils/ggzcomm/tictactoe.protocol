<?xml version="1.0"?>
<ggzcomm engine="tictactoe" version="2">
	<def name="ggz_seat_bot" value="2"/>
	<def name="ggz_seat_player" value="3"/>
	<client>
		<event name="sndmove">
			<data name="move" type="int"/>
		</event>
		<event name="reqsync"/>
	</client>
	<server>
		<event name="msgseat">
			<data name="num" type="int"/>
		</event>
		<event name="msgplayers">
			<sequence count="2">
				<data name="seat" type="int"/>
				<eval>
					<condition name="seat" result="ggz_seat_player"/>
					<condition name="seat" result="ggz_seat_bot"/>
					<data name="name" type="string"/>
				</eval>
			</sequence>
		</event>
		<event name="reqmove"/>
		<event name="rspmove">
			<data name="status" type="byte"/>
		</event>
		<event name="msgmove">
			<data name="move" type="int"/>
		</event>
		<event name="sndsync">
			<data name="turn" type="byte"/>
			<sequence count="9">
				<data name="space" type="byte"/>
			</sequence>
		</event>
		<event name="msggameover">
			<data name="winner" type="byte"/>
		</event>
	</server>
	<protocol>
		<link server="msgseat" followup="msgplayers"/>
		<link client="reqsync" server="sndsync"/>
		<link server="reqmove" client="sndmove"/>
		<link client="sndmove" server="rspmove"/>
	</protocol>
</ggzcomm>

